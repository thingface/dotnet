{
  "version": "0.0.4",
  "authors": [ "Miroslav Božík" ],
  "description": "The thingface .NET client library for connecting devices from .NET applications to thingface gateway",
  "packOptions": {
    "projectUrl": "https://thingface.io",
    "licenseUrl": "https://github.com/thingface/dotnet/blob/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "https://github.com/thingface/dotnet"
    },
    "owners": [ "Thingface" ],
    "tags": [ "Thingface", "MQTT", "IoT", "monitoring", "control", "devices" ],
    "files": {
      "excludeFiles": [
        "bin/Release/netmf43/Thingface.Client.dll",
        "bin/Release/netmf43/Thingface.Client.pdb"
      ],
      "mappings": {
        "lib/netmf43/be/Thingface.Client.dll": "../Thingface.Client.NetMF/bin/Release/be/Thingface.Client.dll",
        "lib/netmf43/be/Thingface.Client.pdb": "../Thingface.Client.NetMF/bin/Release/be/Thingface.Client.pdb",
        "lib/netmf43/be/Thingface.Client.pdbx": "../Thingface.Client.NetMF/bin/Release/be/Thingface.Client.pdbx",
        "lib/netmf43/be/Thingface.Client.pe": "../Thingface.Client.NetMF/bin/Release/be/Thingface.Client.pe",
        "lib/netmf43/le/Thingface.Client.dll": "../Thingface.Client.NetMF/bin/Release/le/Thingface.Client.dll",
        "lib/netmf43/le/Thingface.Client.pdb": "../Thingface.Client.NetMF/bin/Release/le/Thingface.Client.pdb",
        "lib/netmf43/le/Thingface.Client.pdbx": "../Thingface.Client.NetMF/bin/Release/le/Thingface.Client.pdbx",
        "lib/netmf43/le/Thingface.Client.pe": "../Thingface.Client.NetMF/bin/Release/le/Thingface.Client.pe"
      }
    }
  },
  "dependencies": {},
  "frameworks": {
    "net462": {
      "dependencies": {
        "M2Mqtt": "4.3.0",
        "Newtonsoft.Json": "10.0.2"
      }
    },
    "netmf43": {
      "buildOptions": { "define": [ "MF_FRAMEWORK_VERSION_V4_3" ] },
      "frameworkAssemblies": {
        "System": "4.3.1.0",
        "Microsoft.SPOT.Native": "4.3.1.0",
        "Microsoft.SPOT.TinyCore": "4.3.1.0",
        "mscorlib": "4.3.1.0"
      },
      "dependencies": {
        "M2Mqtt": "4.3.0",
        "Json.NetMF": "1.3.0"
      }
    },
    "netstandard1.5": {
      "imports": "dnxcore50",
      "dependencies": {
        "M2MqttDotnetCore": "1.0.0",
        "NETStandard.Library": "1.6.1",
        "Newtonsoft.Json": "10.0.2"
      }
    }
  }
}
